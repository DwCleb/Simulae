{
  "name": "Simulae",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start --reset-cache",
    "reinstall": "rm -rf node_modules && yarn",
    "android": "react-native run-android",
    "android:dev": "cd android && ./gradlew assembleDebug && cd ..",
    "android:build": "cd android && ./gradlew assembleRelease && cd ..",
    "android:clear": "cd android && ./gradlew cleanBuildCache && cd ..",
    "ios": "react-native run-ios --simulator=\"iPhone 11 Pro\"",
    "ios:pod": "cd ios && pod install && cd ..",
    "ios:pod:i": "cd ios && pod install && cd .. && react-native run-ios",
    "ios:build": "cd ios && xcodebuild -workspace App.xcworkspace -scheme App -configuration AppStoreDistribuition archive -archivePath ./Simulae.xcarchive && xcodebuild -exportArchive -archivePath ./Simulae.xcarchive -exportPath ./Simulae.ipa -exportOptionsPlist ./exportOptions.plist",
    "timeline": "git log --all --graph --decorate --oneline",
    "test": "jest",
    "coverage": "jest --coverage --watchAll=false",
    "lint": "eslint --ext .tsx,.ts .",
    "pre": "prettier src/**/*.tsx --write",
    "ts-check": "tsc",
    "format-staged-code": "pretty-quick --staged --pattern \"**/*.ts{,x}\"",
    "static-code-validator": "yarn ts-check && yarn lint",
    "pre-commit-hook": "yarn format-staged-code && yarn static-code-validator",
    "pre-push-hook": "yarn coverage"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.6.2",
    "@react-native-community/clipboard": "^1.2.3",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/toolbar-android": "^0.1.0-rc.2",
    "@react-navigation/drawer": "^5.9.0",
    "@react-navigation/native": "^5.5.1",
    "@react-navigation/stack": "^5.2.6",
    "axios": "^0.19.2",
    "formik": "^2.1.5",
    "i18next": "^19.1.0",
    "immer": "^9.0.6",
    "lottie-ios": "3.1.8",
    "lottie-react-native": "^3.5.0",
    "react": "16.13.1",
    "react-i18next": "^11.7.0",
    "react-native": "0.63.2",
    "react-native-animated-input": "^1.0.1",
    "react-native-config": "^1.0.0",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-image-picker": "^2.3.3",
    "react-native-localize": "^1.4.0",
    "react-native-masked-text": "^1.13.0",
    "react-native-modal": "^11.5.6",
    "react-native-paper": "^4.0.1",
    "react-native-reanimated": "^1.7.1",
    "react-native-restart": "^0.0.13",
    "react-native-safe-area-context": "^3.1.6",
    "react-native-screens": "^2.4.0",
    "react-native-simple-toast": "^1.1.2",
    "react-native-snap-carousel": "^4.0.0-beta.5",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "^12.1.0",
    "react-native-svg-transformer": "^0.14.3",
    "react-native-vector-icons": "^7.0.0",
    "react-native-webview": "^9.2.2",
    "styled-components": "^4.4.1",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.7",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/jest": "^24.0.24",
    "@types/react-native-restart": "^0.0.0",
    "@types/react-native-vector-icons": "^6.4.5",
    "@types/react-test-renderer": "16.9.1",
    "@types/styled-components": "^5.0.1",
    "@types/yup": "^0.26.27",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-react-native-web": "^0.12.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-sonarjs": "^0.5.0",
    "file-loader": "^6.0.0",
    "husky": "^4.0.6",
    "jest": "^25.1.0",
    "jest-sonar-reporter": "^2.0.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "prettier": "^2.0.4",
    "pretty-quick": "^2.0.1",
    "react-art": "^16.13.1",
    "react-docgen-typescript": "^1.16.1",
    "react-test-renderer": "16.13.1",
    "typescript": "^3.7.4",
    "webpack": "^4.42.1"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./jestSetupFile.js"
    ],
    "testResultsProcessor": "jest-sonar-reporter",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.tsx$": "babel-jest",
      "^.+\\.ts$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|@react-native-community)"
    ],
    "testMatch": [
      "<rootDir>/src/**/*.test.tsx",
      "<rootDir>/src/**/*.test.ts"
    ]
  }
}
